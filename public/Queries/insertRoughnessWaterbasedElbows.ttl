PREFIX ex: <https://example.com/ex#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdf: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX fso: <http://w3id.org/fso#> 
PREFIX fpo: <http://w3id.org/fpo#>
PREFIX fo: <http://www.w3.org/1999/XSL/Format#>
PREFIX inst: <https://example.com/inst#> 
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
insert {
  #Adding Roughness
  ?component fpo:roughness ?roughnessID .
  ?roughnessID a fpo:Roughness .
  ?roughnessID fpo:value "3.28083989501312E-07"^^xsd:double .
  ?roughnessID fpo:unit "Meter"^^xsd:string .
}
WHERE {
  values ?componentType {fso:Elbow } ?component a ?componentType .
  #Mathing outlet port of the connected component
  ?system fso:hasComponent ?component .
  ?system fso:hasFlow ?flow .
  ?flow fpo:flowType/fpo:value "Water" .
  
  #Adding Roughness
  BIND(IRI(CONCAT("https://example.com/inst#",STRUUID())) as ?roughnessID)	.
  };
insert {
  #Adding Roughness
  ?component fpo:roughness ?roughnessID .
  ?roughnessID a fpo:Roughness .
  ?roughnessID fpo:value "0.000492126003807298"^^xsd:double .
  ?roughnessID fpo:unit "Meter"^^xsd:string .
  
}
WHERE {
  values ?componentType {fso:Elbow } ?component a ?componentType .
  #Mathing outlet port of the connected component
  ?system fso:hasComponent ?component .
  ?system fso:hasFlow ?flow .
  ?flow fpo:flowType/fpo:value "Air" .
  
